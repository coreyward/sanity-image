{
  "name": "@sanity-image/url-builder",
  "version": "1.0.1",
  "author": "Corey Ward <corey@hey.com>",
  "license": "MIT",
  "repository": "https://github.com/coreyward/sanity-image",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "size": "size-limit",
    "test": "vitest run"
  },
  "devDependencies": {
    "size-limit": "^12.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "2 kB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "2 kB"
    }
  ]
}
