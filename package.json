{
  "name": "sanity-image",
  "version": "0.3.0-alpha.0",
  "author": "Corey Ward <corey@hey.com>",
  "license": "MIT",
  "repository": "https://github.com/coreyward/sanity-image",
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.0.0",
    "@types/probe-image-size": "^7.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.3.5",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "probe-image-size": "^7.2.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-test-renderer": "^18.3.0",
    "rimraf": "^4.4.1",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.21.0"
  },
  "scripts": {
    "build": "rimraf dist && node ./scripts/build.mjs && tsc -p tsconfig.build.json",
    "postbuild": "./scripts/postbuild.sh",
    "size": "size-limit",
    "test": "jest",
    "test:generate-measurements": "yarn build && yarn node tests/buildTestCases.js",
    "lint": "eslint src/**/*{.ts,.tsx}"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/cjs/index.js",
      "limit": "4 kB"
    },
    {
      "path": "dist/mjs/index.js",
      "limit": "3 kB"
    }
  ],
  "packageManager": "pnpm@10.0.0+sha512.b8fef5494bd3fe4cbd4edabd0745df2ee5be3e4b0b8b08fa643aa3e4c6702ccc0f00d68fa8a8c9858a735a0032485a44990ed2810526c875e416f001b17df12b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
